{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:15480",
      "sslPort": 44322
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "SimpleForum.CrossConnection": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "SimpleForum.CrossConnection2": {
      "commandName": "Project",
      "commandLineArgs": "--config ../SimpleForumConfig2.json",
      "environmentVariables": {
        "DbConnectionString": "Server=localhost;User Id=simpleforum;Password=simpleforum_pass;Database=simpleforum2"
      }
    },
    "SimpleForum.CrossConnection Android": {
      "commandName": "Project",
      "commandLineArgs": "--config ../SimpleForumAndroidConfig.json",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "SimpleForum.CrossConnection Android2": {
      "commandName": "Project",
      "commandLineArgs": "--config ../SimpleForumAndroidConfig2.json",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DbConnectionString": "Server=localhost;User Id=simpleforum;Password=simpleforum_pass;Database=simpleforum2"
      }
    }
  }
}
